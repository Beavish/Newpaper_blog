<p id="notice"><%= notice %></p>

<h1>SPORT ______ POSTS</h1>

<table>
  <thead>
    <tr>

      <th>Title</th>
      <th colspan="3"></th>
    
      <th>Public</th>
      <th colspan="3"></th>

      <th>Genre</th>
      <th colspan="3"></th>
      
    </tr>
    
  
  </thead>


  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td colspan="3"><%= post.title %></td>
        <td colspan="3"><%= post.public %></td>
        <td colspan="3"><%= post.Genre %></td>
      
       
      </tr>
    <% end %>
  </tbody>
</table>

<br>


